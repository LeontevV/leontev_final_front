import {
  takeLatest,
  put,
  call,
} from 'redux-saga/effects';

import * as actionTypes from '../constants';
import { loginModal, signInModal } from '../action';
import getPostsRequest from '../../api/getPostsRequest';

function* userSaga() {
  try {
    const { data: payload } = yield call();
    yield put(loginModal());
  } catch (err) {
    yield put(signInModal());
  } 
}

export default function* watcherSaga() {
  yield takeLatest(actionTypes.GET_POST_REQUEST, userSaga);
}